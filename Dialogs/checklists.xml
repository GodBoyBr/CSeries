<?xml version="1.0"?>

<PropertyList>

<checklist>  
    <title>Checks before engine start</title>
<!--
	<item>
		<name>Preflight check</name>	
                <value>COMPLETED</value>
	</item>-->

    <item>
      <name>All Doors</name>
      <value>CLOSED</value>
        <condition>
            <and>
                <equals>
                    <property>sim/model/door-positions/cargo/position-norm</property>
                    <value>0</value>
                </equals>
                <equals>
                    <property>sim/model/door-positions/cargoR/position-norm</property>
                    <value>0</value>
                </equals>
                <equals>
                    <property>sim/model/door-positions/pax-left/position-norm</property>
                    <value>0</value>
                </equals>
                <equals>
                    <property>sim/model/door-positions/pax-right/position-norm</property>
                    <value>0</value>
                </equals>
                <equals>
                    <property>sim/model/door-positions/rear-left/position-norm</property>
                    <value>0</value>
                </equals>
                <equals>
                    <property>sim/model/door-positions/rear-right/position-norm</property>
                    <value>0</value>
                </equals>
            </and>
        </condition>
    </item>

    <item>
      <name>Parking Brake</name>
      <value>APPLIED</value>
      <condition>
        <equals>
          <property>controls/gear/brake-parking</property>
          <value>1</value>
        </equals>
      </condition>
      <binding>
        <command>property-interpolate</command>
        <property>controls/gear/brake-parking</property>
        <value>1</value>
        <time>2.0</time>
      </binding>
    </item>

    <item>
      <name>Batteries</name>
      <value>Both ON</value>
      <condition>
        <and>
            <equals>
                <property>controls/electric/battery-switch</property>
                <value>1</value>
            </equals>
            <equals>
                <property>controls/electric/battery-switch[1]</property>
                <value>1</value>
            </equals>
        </and>
      </condition>
      <binding>
        <command>property-assign</command>
        <property>controls/electric/battery-switch</property>
        <value>1</value>
      </binding>
      <binding>
        <command>property-assign</command>
        <property>controls/electric/battery-switch[1]</property>
        <value>1</value>
      </binding>
    </item>
<!--
    <item>
      <name>Avionics (PFD/MFD)</name>
      <value>Check Running</value>
      <condition>
          <greater-than-equals>
                <property>systems/electrical/left-bus</property>
                <value>10</value>
