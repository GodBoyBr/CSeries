    </item>-->

  </checklist>
   
  <checklist>
    <title>APU Start Procedure</title>
    
    <item>
      <name>Beacon Light</name>
      <value>ON</value>
      <condition>
        <equals>
		<property>controls/lighting/beacon</property>
          <value>1</value>
        </equals>
      </condition>
      <binding>
        <command>property-assign</command>
		<property>controls/lighting/beacon</property>
        <value>1</value>
      </binding>
    </item>
    <item>
      <name>APU Master and Start</name>
      <value>ON</value>
      <condition>
          <and>
            <equals>
		<property>controls/APU/electronic-control-unit</property>
                <value>1</value>
            </equals>
            <equals>
		<property>controls/APU/off-on</property>
            <value>1</value>
            </equals>
        </and>
      </condition>
      <binding>
        <command>property-assign</command>
        <property>controls/APU/electronic-control-unit</property>
        <value>1</value>
      </binding>
      <binding>
        <command>property-assign</command>
        <property>controls/APU/off-on</property>
        <value>1</value>
      </binding>
    </item>

    <item>
      <name>After ~10 seconds</name>
    </item>
    
    <item>
      <name>Check APU</name>
      <value>Running</value>
      <condition>
        <equals>
            <property>engines/apu/running</property>
          <value>1</value>
        </equals>
      </condition>
    </item>

    <item>
      <name>APU Generator</name>
      <value>ON</value>
      <condition>
        <equals>
            <property>controls/electric/APU-generator</property>
          <value>1</value>
        </equals>
      </condition>
      <binding>
        <command>property-assign</command>
            <property>controls/electric/APU-generator</property>
        <value>1</value>
      </binding>
    </item>

    <item>
      <name>APU Bleed</name>
      <value>ON</value>
      <condition>
        <equals>
		<property>controls/pneumatic/APU-bleed</property>
          <value>1</value>
        </equals>
      </condition>
      <binding>
        <command>property-assign</command>
		<property>controls/pneumatic/APU-bleed</property>
        <value>1</value>
      </binding>
    </item>

  </checklist>

    <checklist>
        <title>Engines start procedure</title>
        
        
        <item>
        <name>APU Start Procedure</name>
        <value>COMPLETED</value>
        <condition>
            <and>
                <equals>
                    <property>engines/apu/running</property>
                <value>1</value>
                </equals>
                <equals>
                    <property>controls/electric/APU-generator</property>
                    <value>1</value>
                </equals>
                <equals>
                    <property>controls/pneumatic/APU-bleed</property>
                    <value>1</value>
                </equals>
            </and>
        </condition>
    </item>

        <item>
            <name>Strobe Light</name>
            <value>ON</value>
            <condition>
                <equals>
                    <property>controls/lighting/strobe</property>
                    <value>1</value>
                </equals>
            </condition>
            <binding>
                <command>property-assign</command>
                <property>controls/lighting/strobe</property>
                <value>1</value>
            </binding>
        </item>
    <item>
      <name>Navigation Light</name>
      <value>ON</value>
      <condition>
        <equals>
		<property>controls/lighting/nav-lights</property>
          <value>1</value>
        </equals>
      </condition>
      <binding>
        <command>property-assign</command>
		<property>controls/lighting/nav-lights</property>
        <value>1</value>
      </binding>
    </item>
    <item>
        <name>Throttle</name>
        <value>IDLE</value>
        <marker>
            <x-m>-0.7256</x-m>
            <y-m>-0.0901</y-m>
            <z-m>0.0892</z-m>
            <scale>3.11</scale>
        </marker>
        <condition>
            <less-than>
            <property>/controls/engines/engine[0]/throttle</property>
            <value>0.1</value>
            </less-than>
            <less-than>
            <property>/controls/engines/engine[1]/throttle</property>
            <value>0.1</value>
            </less-than>
        </condition>
        <binding>
            <command>property-interpolate</command>
            <property>/controls/engines/engine[0]/throttle</property>
            <value>0.05</value>
            <time>1.0</time>
        </binding>
        <binding>
            <command>property-interpolate</command>
            <property>/controls/engines/engine[1]/throttle</property>
            <value>0.05</value>
            <time>1.0</time>
        </binding>
    </item>

    <item>
      <name>Left Engine Master Switch (Pedestal)</name>
      <value>ON</value>
      <condition>
          <equals>
		<property>/controls/engines/engine[0]/cutoff</property>
                <value>0</value>
            </equals>
      </condition>
      <binding>
        <command>property-assign</command>
		<property>/controls/engines/engine[0]/cutoff</property>
                <value>0</value>
      </binding>
    </item>

    <item>
      <name>Engine Start Knob</name>
      <value>LEFT</value>
      <condition>
          <equals>
		<property>controls/engines/start1</property>
                <value>1</value>
            </equals>
      </condition>
      <binding>
        <command>property-assign</command>
        <property>controls/engines/start1</property>
        <value>1</value>
      </binding>
			<binding>
				<command>nasal</command>
				<script>cseries.engine1.start()</script>
			</binding>
    </item>
    
    <item>
      <name>Observe Left N1</name>
    </item>

    <item>
        <name>Left N1</name>
      <value>Greater or equals 22%</value>
      <condition>
          <greater-than-equals>
		<property>engines/engine/rpm</property>
                <value>22</value>
            </greater-than-equals>
      </condition>
    </item>
    

    <item>
      <name>Engine Start Knob</name>
      <value>OFF</value>
      <condition><and>
          <equals>
		<property>controls/engines/start1</property>
                <value>0</value>
            </equals>
          <equals>
		<property>/controls/engines/start2</property>
                <value>0</value>
                </equals></and>
      </condition>
      <binding>
        <command>property-assign</command>
        <property>controls/engines/start1</property>
        <value>0</value>
      </binding>
      <binding>
        <command>property-assign</command>
        <property>/controls/engines/start2</property>
        <value>0</value>
      </binding>
    </item>
    
    <item>
      <name>Right Engine Master Switch (Pedestal)</name>
      <value>ON</value>
      <condition>
          <equals>
		<property>/controls/engines/engine[1]/cutoff</property>
                <value>0</value>
            </equals>
      </condition>
      <binding>
        <command>property-assign</command>
		<property>/controls/engines/engine[1]/cutoff</property>
                <value>0</value>
      </binding>
    </item>

    <item>
      <name>Engine Start Knob</name>
      <value>RIGHT</value>
      <condition>
          <equals>
		<property>/controls/engines/start2</property>
                <value>1</value>
            </equals>
      </condition>
      <binding>
        <command>property-assign</command>
        <property>/controls/engines/start2</property>
        <value>1</value>
      </binding>
			<binding>
				<command>nasal</command>
				<script>cseries.engine2.start()</script>
			</binding>
    </item>
    
    <item>
      <name>Observe Right N1</name>
    </item>

    <item>
        <name>Right N1</name>
      <value>Greater or equals 22%</value>
      <condition>
          <greater-than-equals>
		<property>engines/engine[1]/rpm</property>
                <value>22</value>
            </greater-than-equals>
      </condition>
    </item>

    <item>
      <name>Engine Start Knob</name>
      <value>NEUTRAL</value>
      <condition><and>
          <equals>
		<property>controls/engines/start1</property>
                <value>0</value>
            </equals>
          <equals>
		<property>/controls/engines/start2</property>
                <value>0</value>
                </equals></and>
      </condition>
      <binding>
        <command>property-assign</command>
        <property>controls/engines/start1</property>
        <value>0</value>
      </binding>
      <binding>
        <command>property-assign</command>
        <property>/controls/engines/start2</property>
        <value>0</value>
      </binding>
    </item>
    
    <item>
      <name>Engine Generators</name>
      <value>LEFT and RIGHT ON</value>
      <condition>
          <equals>
		<property>controls/electric/engine/generator</property>
                <value>1</value>
            </equals>
          <equals>
		<property>controls/electric/engine[1]/generator</property>
                <value>1</value>
            </equals>
      </condition>
      <binding>
        <command>property-assign</command>
		<property>controls/electric/engine/generator</property>
        <value>1</value>
      </binding>
      <binding>
        <command>property-assign</command>
		<property>controls/electric/engine[1]/generator</property>
        <value>1</value>
      </binding>
    </item>
    
    <item>
      <name>APU Bleed</name>
      <value>OFF</value>
      <condition>
        <equals>
		<property>controls/pneumatic/APU-bleed</property>
          <value>0</value>
        </equals>
      </condition>
      <binding>
        <command>property-assign</command>
		<property>controls/pneumatic/APU-bleed</property>
        <value>0</value>
      </binding>
    </item>
    
    
    <item>
      <name>APU Generator</name>
      <value>OFF</value>
      <condition>
        <equals>
            <property>controls/electric/APU-generator</property>
          <value>1</value>
        </equals>
      </condition>
      <binding>
        <command>property-assign</command>
            <property>controls/electric/APU-generator</property>
        <value>1</value>
      </binding>
    </item>
    
    <item>
      <name>APU Master</name>
      <value>OFF</value>
      <condition>
          <and>
            <equals>
		<property>controls/APU/electronic-control-unit</property>
