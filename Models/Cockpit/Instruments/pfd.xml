       <max>359</max>
       <wrap>true</wrap>
      </binding>
      <binding>
        <condition>
          <property>/instrumentation/zkv1000/PFD/status</property>
        </condition>
        <command>nasal</command>
        <script>
          foreach (var c; keys(zkv1000.flightdeck))
            if (zkv1000.flightdeck[c].role == 'PFD')
              zkv1000.flightdeck[c].display.updateHDG();
        </script>
      </binding>
    </action>
