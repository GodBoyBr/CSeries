      </binding>
      <binding>
        <condition>
          <property>/instrumentation/zkv1000/PFD/status</property>
        </condition>
        <command>nasal</command>
        <script>
          foreach (var c; keys(zkv1000.flightdeck))
            zkv1000.flightdeck[c].display.updateNAV({set:1});
        </script>
      </binding>
    </shift-action>
  </animation>

