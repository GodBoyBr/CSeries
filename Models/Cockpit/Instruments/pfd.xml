        <factor>10</factor>
        <min>0</min>
        <max>359</max>
        <wrap>true</wrap>
      </binding>
      <binding>
        <condition>
          <property>/instrumentation/zkv1000/PFD/status</property>
          <not>
            <equals>
              <property>/instrumentation/zkv1000/cdi/source</property>
              <value>GPS</value>
